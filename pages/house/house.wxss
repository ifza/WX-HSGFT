/* 引入组件样式 */
@import '../components/star/star.wxss';

/* pages/house/house.wxss */
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__content{
    padding-top: 60px;
}

/* 自定义样式 */
.weui-tab__content{
  padding-top:0;
}
.weui-navbar__item{
    position: relative;
    border-right:1px solid #CCC;
    height:40px;
    line-height: 40px;
    padding:0;
    font-size:20px;
}
.weui-navbar__item:last-child{
    border-right:none;
}
.weui-bar__item_on.active{
    color:#000;
    background-color:#EAEAEA;
}
.weui-navbar__item .arrow{
    position: absolute;
    right:10px;
    width:11px;
    height:8px;
    background-image:url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size:100%;
    top:50%;
    
}
.weui-navbar__item .arrow.star{
    border:0px solid transparent;
}
.weui-navbar__item .arrow.top{
    margin-top:-10px;
}
.weui-navbar__item .star.top{
}
.weui-navbar__item .star.bottom{

}
.weui-navbar__item .arrow.bottom{
    transform: rotate(180deg);
    margin-bottom:-10px;
    border-top:none;
}
.weui-navbar__item.start.order .top,
.weui-navbar__item.start.desc .bottom{
    background-image: url('data:image/png;base64,')
}
.weui-navbar__item.start.order .top{  
}
.weui-navbar__item .arrow.area{
    margin-top:-5px;
}
.weui-tab__panel{
    padding-top:40px;
}

/* panel */
.weui-panel::before,
.weui-panel::after{
    content:'';
    border:none;
}
.weui-panel__hd{
    color:#838383;
}
.weui-panel__hd::after,
.weui-media-box::before,
.weui-panel__ft .weui-cell::before{
    left:0;
}
.weui-media-box__title{
    position:relative;
    display:block;
    overflow: visible;
}
.weui-media-box__title .title-txt{
    float:left;
}
.weui-media-box__title .status{
    display:inline-block;
    position:relaitve;
    font-size:10px;
    padding:0 2px;
    margin-left:10rpx;
    color:#fff;
    border-radius:3px;
    letter-spacing: 3px;
}
.weui-media-box__title .online{
    background-color:#27A700;
}
.weui-media-box__title .without{
    background-color:#FF0000;
}
.weui-media-box__title .star{
    position:absolute;
    top:50%;
    right:0px;
    display:block;
    width:75px;
    height:13px;
    margin-top:-6.5px;
    font-size:10px;
    padding:0 2px;
    color:#fff;
    border-radius:3px;
    letter-spacing: 3px;
}
.weui-media-box__title .star.score4{
    /* background-image:url('../../assets/images/score4.jpg');
    background-repeat: no-repeat;
    background-size:100%; */
}
.weui-cell__bd{
    color:#032d7b;    
}
.weui-media-box__title{
    height:33px;
}
/* picker样式修改 */
.weui-navbar__item{
    position:relative;
}
.picker-wrap{
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
}
.filter-area{
    width:100%;
    height:100%;
}
.filter-area .picker{
    font-size:15px;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    word-wrap:normal;    
}
.empty-list{
    display:block;
    width:100%;
    text-align: center;
    font-size:16px;
    color:#f00;
}