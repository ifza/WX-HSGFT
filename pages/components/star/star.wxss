/* 评分星星组件样式 */
.star-wrap{
  position:absolute;
  top:50%;
  right:0px;
  display:block;
  width:75px;
  height:13px;
  margin-top:-6.5px;
  font-size:10px;
  padding:0 2px;
  color:#fff;
  border-radius:3px;
  letter-spacing:3px;
}
.star-wrap.static{
  position:relative;
  display:inline-block;
  margin-left:10px;
  width:79px;
  height:13px;
}
.star-wrap .list{
  position:relative;
  display:flex;
  flex-wrap:nowrap;
  width:100%;
  height:100%;
}
.star-wrap .list .item{
  display:inline-block;
  flex:1;
  height:1;
  background-size:78%;
  background-position:center;
  background-repeat:no-repeat;
}
.star-wrap .list .item.full{
  background-image:url('data:image/png;base64,');
}
.star-wrap .list .item.empty{
  background-image:url('data:image/jpeg;base64,');
}
.star-wrap .empty-list{
  position:absolute;
  display:flex;
  flex-wrap: nowrap;
  top:0;
  left:0;
  width:100%;
  height:100%;
}
.star-wrap .empty-list .item{
  display:inline-block;
  flex:1;
  height:1;
  background-size:78%;
  background-position:center;
  background-repeat:no-repeat;
}
.star-wrap .empty-list .item{
  background-image:url('data:image/jpeg;base64,');
}